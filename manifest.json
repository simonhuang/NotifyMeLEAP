
{
	
"name":"NotifyMeLEAP",
"version":"0.1",
"description":"Notifications for Leap!",
"manifest_version":2,
"web_accessible_resources": [
	"prototype/sidebar.html",
	"prototype/css/*.css",
	"prototype/js/*.js",
	"prototype/assets/*",
	"prototype/img/*"
],
"content_scripts":[
   {
	   "matches":["*://*/*"],
	   "run_at":"document_end",
	   "js":["leap-0.6.2.js","prototype/js/jquery.js","prototype/js/bootstrap.min.js","prototype/js/google-gmail.js","script.js"]
      }]

}